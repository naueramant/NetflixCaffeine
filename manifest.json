{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "NetflixCaffeine",
  "description": "Prevents your computer from going to sleep while watching a movie or series on Netflix.",
  "version": "1.0.4",

  "icons": { 
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png" 
  },

  "permissions": [
    "tabs",
    "power"
  ],

  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  }
}